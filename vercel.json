{
    "outputDirectory": "dist",
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "/"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/delayed-notifications",
            "schedule": "0 3 * * 1"
        }
    ]
}